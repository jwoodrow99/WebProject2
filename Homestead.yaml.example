ip: "192.168.10.10"
memory: 2048
cpus: 2
provider: virtualbox

authorize: ~/.ssh/id_rsa.pub

keys:
    - ~/.ssh/id_rsa

folders:
    -
        map: '~/Workspace/NAME/NAME-app'
        to: /home/vagrant/app

sites:
    -
        map: .test
        to: /home/vagrant/app/public

databases:
    - NAME

name: NAME

hostname: NAME

